{
  "name": "codeswarm",
  "version": "1.0.0",
  "description": "Autonomous code generation system with multi-agent coordination",
  "main": "src/index.js",
  "bin": {
    "codeswarm": "./src/cli/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint src/ --fix",
    "setup": "node src/cli/commands/setup.js",
    "build": "npm ci && npm test && npm run lint"
  },
  "keywords": [
    "ai",
    "code-generation",
    "multi-agent",
    "autonomous",
    "claude",
    "swarm"
  ],
  "author": "CodeSwarm Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.9",
    "@anthropic-ai/sdk": "^0.65.0",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.0",
    "jest": "^29.7.0",
    "ora": "^7.0.1",
    "simple-git": "^3.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
